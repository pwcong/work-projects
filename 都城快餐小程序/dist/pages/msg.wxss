.main{height:100%;width:100%;-webkit-box-sizing:border-box;box-sizing:border-box;overflow:auto;position:relative}.main view{text-align:center;font-weight:bold}.main view .btn{margin:0 auto;width:360rpx;height:70rpx;border:1px solid #d30a0a;border-radius:8rpx;line-height:70rpx;color:#d30a0a;font-weight:normal;-webkit-transition:color 0.1s, background-color 0.1s;transition:color 0.1s, background-color 0.1s}.main view .btn:active{color:white;background-color:#d30a0a}
