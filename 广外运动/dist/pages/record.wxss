.nav{width:100%;height:132rpx;border-bottom:1px solid #ADACAC;font-size:14px;color:#4A4A4A;letter-spacing:-0.34px}.nav .nav-main{width:432rpx;height:66rpx;border:1px solid #F5A623;border-radius:33rpx;box-sizing:border-box;margin:0 auto;overflow:hidden;font-size:12px}.nav .nav-tips{margin-top:20rpx;font-size:11px;color:#4A4A4A;letter-spacing:-0.26px;justify-content:center}.nav .col{justify-content:center;z-index:100;color:#F5A623;transition:color 0.2s}.nav .col.active{color:white}.nav .mask{position:absolute;top:0;background-color:#F5A623;width:50%;height:100%;border-radius:30rpx;transition:left 0.2s}.main{position:relative;width:100%;height:calc(100% - 132rpx);overflow:hidden;background-color:#F1F1F1;box-sizing:border-box}.main .record{height:100%;overflow:hidden}.main .record .record-section{display:none}.main .record .record-section.active{display:initial}.main .record .record-item{width:100%;background-color:white;box-sizing:border-box;padding:20rpx 0;margin-bottom:10rpx}.main .record .record-item .record-item-head{justify-content:space-between;font-size:13px;color:#4A4A4A;letter-spacing:-0.31px;box-sizing:border-box;padding:0 40rpx;margin-bottom:40rpx}.main .record .record-item .record-item-head image{width:30rpx;height:30rpx;margin-right:10rpx}.main .record .record-item .record-item-head .left{justify-content:flex-start}.main .record .record-item .record-item-head .right{justify-content:flex-end;flex:2}
