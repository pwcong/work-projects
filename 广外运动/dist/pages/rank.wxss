.nav{width:100%;height:106rpx;background-color:white;box-shadow:0 2px 4px 0 rgba(148,148,148,0.41);font-size:14px;color:#4A4A4A;letter-spacing:-0.34px}.nav .col{justify-content:center}.nav .divider{width:1px;height:30rpx;background-color:#09BB07}.nav .nav-index{position:absolute;bottom:0;left:0;width:100%;height:6rpx}.nav .nav-index .nav-index-flag{position:relative;transition:left 0.2s;left:0;height:100%}.nav .nav-index .nav-index-flag view{margin:0 auto;width:50%;height:100%;background-color:#09BB07}.main{position:relative;width:100%;height:calc(100% - 106rpx);overflow:hidden;background-color:#F1F1F1;box-sizing:border-box}.main .rank{height:100%}.main .rank .rank-section{overflow:auto}.main .rank .rank-section .rank-item{box-sizing:border-box;height:180rpx;background-color:white;box-shadow:0 2px 4px 0 rgba(148,148,148,0.41);align-items:center;padding:46rpx 30rpx;justify-content:space-between}.main .rank .rank-section .rank-item .rank-item-col{display:flex;align-items:center;height:100%;flex-flow:row nowrap}.main .rank .rank-section .rank-item.rank-self{margin-bottom:14rpx}.main .rank .rank-section .rank-item.rank-self .score{color:#09BB07}.main .rank .rank-section .rank-item .name{font-size:17px;color:#6F6F6F;letter-spacing:0.23px}.main .rank .rank-section .rank-item .score{color:#4A4A4A;font-size:27px;letter-spacing:-0.65px;margin-right:34rpx}.main .rank .rank-section .rank-item .avatar{margin-right:26rpx;width:90rpx;height:90rpx;border-radius:45rpx}.main .rank .rank-section .rank-item .step{font-size:25px;color:#4A4A4A;letter-spacing:-0.6px;margin-right:14rpx}.main .rank .rank-section .rank-item .icon-step{width:48rpx;height:50rpx}.main .rank .rank-section .rank-item .icon-medal{width:54rpx;height:72rpx;margin-right:20rpx}
