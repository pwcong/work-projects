.toolbar{height:120rpx;width:100%;background-color:white;box-sizing:border-box;padding:24rpx 30rpx;box-shadow:0 1px 2px 0 rgba(0,0,0,0.11)}.toolbar .wrap{width:100%;height:100%;background-color:#e5e5e5;border:1px solid #e5e5e5;border-radius:8rpx;display:flex;flex-flow:row nowrap;align-items:center;box-sizing:border-box;padding:0 30rpx;transition:background-color 0.1s}.toolbar .wrap image{margin-right:8rpx;width:40rpx;height:40rpx}.toolbar .wrap input{flex:1;font-size:14px;width:100%}.main{flex:1;box-sizing:border-box;overflow:auto;position:relative}.main .info{box-sizing:border-box;padding:30rpx 40rpx;display:flex;flex-flow:row wrap}.main .info .tips{width:100%;font-size:16px;color:#9b9b9b;margin-bottom:20rpx}.main .info .location{padding:18rpx 34rpx;font-size:16px;border:1px solid #9B9B9B;border-radius:8rpx;background-color:white}.main .shops{background-color:white}.main .shops .tips{width:100%;height:94rpx;box-sizing:border-box;padding:0 40rpx;line-height:94rpx;font-size:16px;border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5}.main .shops .wrap{box-sizing:border-box;padding:0 40rpx;width:100%}.main .shops .wrap .item{height:120rpx;width:100%;line-height:120rpx;font-size:15px}.main .shops .wrap .item:not(:last-child){border-bottom:1px solid #e5e5e5}
