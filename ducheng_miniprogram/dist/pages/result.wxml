
    <view class="container">
        <view class="main">
            <view class="result" style="margin: 100rpx 0 200rpx 0; background-image: url({{bgUrl}});">
                <view class="result-wrap">
                    <view>
                        <icon type="{{type}}" size="70"/>
                    </view>
                    <view class="result-msg {{type == 'success' ? 'result-msg-success' : 'result-msg-warn'}}">{{msg}}</view>
                    <view class="result-content" style="font-size: {{type == 'success' ? '32px' : '18px'}}">{{content}}</view>
                </view>

            </view>
    
            <view>
                <view class="btn" bindtap="handleCheckButtonClick">чбошод</view>
            </view>
        </view>
    </view>