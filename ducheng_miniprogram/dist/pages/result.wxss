.main{flex:1;box-sizing:border-box;overflow:auto;padding:0 30rpx;position:relative}.main view{text-align:center;font-weight:bold;position:relative}.main view .btn{margin:0 auto;width:360rpx;height:70rpx;border:1px solid #D30A0A;border-radius:8rpx;line-height:70rpx;color:#D30A0A;font-weight:normal;transition:color 0.1s, background-color 0.1s}.main view .btn:active{color:white;background-color:#D30A0A}.main .result{position:relative;width:100%;background-repeat:no-repeat;height:696rpx;background-size:100% 100%}.main .result .result-wrap{width:100%;height:100%;position:absolute;left:0;top:0;box-sizing:border-box;padding:120rpx 50rpx 100rpx 50rpx}.main .result .result-wrap .result-msg{font-size:16px;padding:40rpx 0;border-bottom:1px dashed #c6c6c6}.main .result .result-wrap .result-msg-success{color:#09bb07}.main .result .result-wrap .result-msg-warn{color:#e64240}.main .result .result-wrap .result-content{margin-top:60rpx;font-size:18px}
