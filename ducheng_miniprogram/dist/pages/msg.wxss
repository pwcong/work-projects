.main{flex:1;box-sizing:border-box;overflow:auto;position:relative}.main view{text-align:center;font-weight:bold}.main view .btn{margin:0 auto;width:360rpx;height:70rpx;border:1px solid #D30A0A;border-radius:8rpx;line-height:70rpx;color:#D30A0A;font-weight:normal;transition:color 0.1s, background-color 0.1s}.main view .btn:active{color:white;background-color:#D30A0A}
